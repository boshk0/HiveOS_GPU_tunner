if [ -z "$1" ]
then
  if [ -f "/profile/current.profile" ]
  then
    head current.profile
    head $(head /profile/current.profile)
  else
    echo No profile is set
    exit 0
  fi
else
  if [ -f "$1" ]
  then
    echo $1
    head /profile/$1
  else
    echo Profile with name '$1' does not exist
    exit 0
  fi
fi